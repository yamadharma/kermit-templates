author = {Kulyabov, Dmitry Sergeevich},
booktitle = {International Conference on Mathematical Modeling and Computational Physics},
file = {:home/dharma/me/Публикациии/bib/files/2015/mmcp-2015_maxwell.pdf:pdf},
